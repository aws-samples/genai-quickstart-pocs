{
  "name": "investment-ai-agent",
  "version": "1.0.0",
  "description": "Multi-Agent AI System for Investment Research & Recommendations using Amazon Bedrock",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --config src/__tests__/integration/jest.integration.config.js",
    "test:integration:watch": "jest --config src/__tests__/integration/jest.integration.config.js --watch",
    "test:integration:standalone": "jest --config src/__tests__/integration/jest.standalone.config.js",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:runner": "node scripts/test-runner.js",
    "test:models": "node scripts/test-runner.js models",
    "test:utils": "node scripts/test-runner.js utils",
    "test:services": "node scripts/test-runner.js services",
    "test:controllers": "node scripts/test-runner.js controllers",
    "test:coverage": "node scripts/test-runner.js coverage",
    "test:watch": "node scripts/test-runner.js watch",
    "test:ci": "npm run test:unit && npm run test:integration",
    "test:security": "jest --testNamePattern=\"Security Testing Suite\"",
    "test:compliance": "jest --testNamePattern=\"Compliance Verification Testing Suite\"",
    "test:privacy": "jest --testNamePattern=\"Data Privacy Validation Testing Suite\"",
    "test:security-all": "npm run test:security && npm run test:compliance && npm run test:privacy",
    "cdk": "cdk",
    "start": "ts-node src/api/test-server.ts",
    "dev": "nodemon --exec ts-node src/api/test-server.ts",
    "demo:deploy": "node scripts/demo-deploy.js",
    "demo:deploy:mock": "node scripts/demo-deploy.js --mock",
    "demo:deploy:minimal": "node scripts/demo-deploy.js --minimal --mock",
    "demo:deploy:cost": "node scripts/demo-deploy.js --cost-optimized",
    "demo:cleanup": "npx cdk destroy investment-ai-agent-demo",
    "demo:local": "node scripts/local-demo.js",
    "validate:deployment": "node scripts/deployment-validation.js",
    "deploy:dev": "node scripts/deploy.js dev",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:prod": "node scripts/deploy.js prod blue-green",
    "test:deployment:dev": "NODE_ENV=dev node scripts/post-deployment-tests.js",
    "test:deployment:staging": "NODE_ENV=staging node scripts/post-deployment-tests.js",
    "test:deployment:prod": "NODE_ENV=prod node scripts/post-deployment-tests.js",
    "test:smoke:staging": "NODE_ENV=staging npm run test:integration:standalone",
    "test:smoke:prod": "NODE_ENV=prod npm run test:integration:standalone",
    "deployment:backup": "echo 'Creating deployment backup...' && aws s3 sync . s3://deployment-backups-$(date +%Y%m%d%H%M%S) --exclude node_modules/",
    "deployment:rollback": "echo 'Rolling back deployment...' && npx cdk deploy --rollback",
    "deployment:finalize": "echo 'Finalizing blue-green deployment...'",
    "deployment:notify": "echo 'Sending deployment notification...'",
    "monitoring:setup": "node scripts/setup-monitoring.js",
    "monitoring:setup:prod": "NODE_ENV=prod node scripts/setup-monitoring.js",
    "monitoring:dashboard": "echo 'Opening CloudWatch dashboard...' && open https://console.aws.amazon.com/cloudwatch/home#dashboards:"
  },
  "keywords": [
    "aws",
    "cdk",
    "bedrock",
    "ai",
    "investment"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/connect": "^3.4.38",
    "@types/jest": "^29.5.5",
    "@types/mime": "^3.0.4",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.8.0",
    "@types/serve-static": "^1.15.8",
    "@types/supertest": "^6.0.3",
    "aws-cdk": "2.100.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.848.0",
    "@aws-sdk/client-bedrock-runtime": "^3.848.0",
    "@types/aws-serverless-express": "^3.3.10",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.8",
    "aws-cdk-lib": "2.100.0",
    "aws-jwt-verify": "^5.1.0",
    "aws-sdk": "^2.1574.0",
    "aws-serverless-express": "^3.4.0",
    "bcrypt": "^6.0.0",
    "constructs": "^10.3.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  }
}
