{
  "name": "@agentic-risk-assessment/infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure code",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint lib/**/*.ts",
    "lint:fix": "eslint lib/**/*.ts --fix",
    "clean": "rm -rf lib",
    "typecheck": "tsc --noEmit",
    "cdk": "cdk",
    "deploy:dev": "cdk deploy --context environment=dev",
    "deploy:staging": "cdk deploy --context environment=staging",
    "deploy:prod": "cdk deploy --context environment=prod",
    "diff": "cdk diff",
    "synth": "cdk synth"
  },
  "dependencies": {
    "@agentic-risk-assessment/shared": "^1.0.0",
    "aws-cdk-lib": "^2.110.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "aws-cdk": "^2.110.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  }
}